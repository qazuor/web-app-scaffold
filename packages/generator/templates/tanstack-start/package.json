{
    "name": "{{appName}}",
    "private": true,
    "version": "0.1.0",
    "description": "{{description}}",
    "type": "module",
    "scripts": {
        "dev": "npx @tanstack/start@latest dev",
        "build": "npx @tanstack/start@latest build",
        "start": "node ./.tanstack/build/server/index.js",
        "lint": "biome lint --config-path=./config/biome.json ./src",
        "format": "biome format --config-path=./config/biome.json --write ./src",
        "check": "biome check --config-path=./config/biome.json --apply ./src",
        "generate-routes": "npx @tanstack/router-cli@latest generate"
    },
    "dependencies": {
        "@tanstack/react-query": "^5.13.4",
        "@tanstack/react-router": "^1.15.10",
        "@tanstack/start": "^1.116.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@repo/config": "workspace:*",
        "@types/node": "^22.14.0",
        "typescript": "^5.8.2",
        "@tanstack/router-cli": "^1.116.0",
        "@tanstack/router-devtools": "^1.15.10",
        "@tanstack/router-vite-plugin": "^1.15.10",
        "@types/react": "^18.2.45",
        "@types/react-dom": "^18.2.18",
        "autoprefixer": "^10.4.16",
        "postcss": "^8.4.32",
        "tailwindcss": "^3.4.0"
    }
}
