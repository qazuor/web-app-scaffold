{
    "$schema": "../../framework.schema.json",
    "name": "react-router",
    "displayName": "React Router Web App",
    "description": "Modern React application with React Router",
    "defaultAppName": "react-router-app",
    "defaultAppDescription": "A modern web application built with React Router",
    "dependencies": [
        {
            "name": "@react-router/node",
            "version": "^7.5.3"
        },
        {
            "name": "@react-router/serve",
            "version": "^7.5.3"
        },
        {
            "name": "isbot",
            "version": "^5.1.27"
        },
        {
            "name": "react",
            "version": "^19.1.0"
        },
        {
            "name": "react-dom",
            "version": "^19.1.0"
        },
        {
            "name": "react-router",
            "version": "^7.5.3"
        },
        {
            "name": "@repo/logger",
            "version": "workspace:*"
        }
    ],
    "devDependencies": [
        {
            "name": "@repo/config",
            "version": "workspace:*"
        },
        {
            "name": "@react-router/dev",
            "version": "^7.5.3"
        },
        {
            "name": "@tailwindcss/vite",
            "version": "^4.1.4"
        },
        {
            "name": "@types/node",
            "version": "^20.10.0"
        },
        {
            "name": "@types/react",
            "version": "^19.1.2"
        },
        {
            "name": "@types/react-dom",
            "version": "^19.1.2"
        },
        {
            "name": "tailwindcss",
            "version": "^4.1.4"
        },
        {
            "name": "typescript",
            "version": "^5.8.3"
        },
        {
            "name": "vite",
            "version": "^6.3.3"
        },
        {
            "name": "vite-tsconfig-paths",
            "version": "^5.1.4"
        }
    ],
    "scripts": [
        {
            "name": "dev",
            "command": "react-router dev"
        },
        {
            "name": "build",
            "command": "react-router build"
        },
        {
            "name": "start",
            "command": "react-router-serve ./build/server/index.js"
        },
        {
            "name": "typecheck",
            "command": "react-router typegen && tsc"
        },
        {
            "name": "lint",
            "command": "biome check ."
        },
        {
            "name": "format",
            "command": "biome format --write ."
        }
    ],
    "hasUI": true,
    "addBiome": true,
    "addTesting": true,
    "testingDependencies": [
        {
            "name": "vitest",
            "version": "^1.3.0"
        },
        {
            "name": "@vitest/coverage-v8",
            "version": "^1.3.0"
        },
        {
            "name": "@testing-library/react",
            "version": "^14.1.2"
        },
        {
            "name": "@testing-library/jest-dom",
            "version": "^6.1.5"
        }
    ],
    "testingScripts": [
        {
            "name": "test",
            "command": "vitest run"
        },
        {
            "name": "test:watch",
            "command": "vitest"
        },
        {
            "name": "test:coverage",
            "command": "vitest run --coverage"
        }
    ]
}
